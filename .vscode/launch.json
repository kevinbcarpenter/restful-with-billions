{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Restful",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/gcc/Debug/restful",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/bin/gcc/Debug",
